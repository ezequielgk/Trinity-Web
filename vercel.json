{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/wiki", "destination": "/pages/wiki.html" },
    { "source": "/contributors", "destination": "/pages/contributors.html" },
    { "source": "/faq", "destination": "/pages/faq.html" },
    { "source": "/api/(.*)", "destination": "/api/$1" }
  ]
}